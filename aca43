
//խնդիր43
#include <iostream>

class Matrix {
private:
    int data[3][3]; 

public:
    
    void input() {
        std::cout << "3x3 matric\n";
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                std::cin >> data[i][j];
            }
        }
    }

    void display() {
        std::cout << "Matric\n";
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                std::cout << data[i][j] << " ";
            }
            std::cout << std::endl;
        }
    }
};

int main() {
    Matrix A;
    A.input();
    A.display();
    return 0;
}
